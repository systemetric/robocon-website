{
  "name": "robocon-site",
  "version": "1.0.0",
  "main": "README.md",
  "private": true,
  "scripts": {
    "build": "yarn images:sizes && yarn build:vuepress && yarn build:functions",
    "build:vuepress": "vuepress build && cp ./.vuepress/dist/assets/js/admin.*.js ./.vuepress/dist/assets/js/admin.js",
    "build:functions": "cd ./.vuepress/functions/gosrc && make build",
    "images:compress": "node ./.build/imageCompressor.js",
    "images:sizes": "node ./.build/imageSizes.js",
    "images:thumbnails": "node ./.build/imageThumbnails.js",
    "images:zip": "node ./.build/imageZipCreator.js",
    "dev": "yarn images:sizes && vuepress dev",
    "netlify:dev": "netlify dev",
    "netlify:dev:live": "netlify dev --live"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@types/archiver": "^2.1.3",
    "@types/auth0-js": "^9.10.2",
    "@types/image-size": "^0.7.0",
    "@types/quill": "^2.0.2",
    "@types/sharp": "^0.21.2",
    "@vue/eslint-config-prettier": "^3.0.0-rc.4",
    "archiver": "^3.0.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "dotenv": "^7.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-vue": "^4.6.0",
    "front-matter": "^2.3.0",
    "guage": "^0.0.0",
    "html-loader": "^0.5.5",
    "image-size": "^0.7.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.2",
    "request": "^2.88.0",
    "request-image-size": "^2.1.0",
    "request-promise-native": "^1.0.7",
    "sass-loader": "^7.0.3",
    "sharp": "^0.21.3",
    "vuepress": "^0.14.10",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "dependencies": {
    "auth0-js": "^9.10.4",
    "filesize": "^4.1.2",
    "highlight.js": "^9.15.8",
    "node-fetch": "^2.3.0",
    "nprogress": "0.2.0",
